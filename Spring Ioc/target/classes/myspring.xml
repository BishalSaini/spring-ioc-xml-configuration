<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here --> 
    <bean id="myemployee" class="com.jsp.employee.Employee"> 
    <property name="id" value="6"></property> 
    <property name="name" value="vishu"></property> 
    <property name="email" value="vishu@mail.com"></property>
    
    
    </bean> 
    
    <bean id="mystudent" class="com.jsp.student.constructorinjection.Student"> 
    <constructor-arg index="0" value="06"> </constructor-arg>
    <constructor-arg index="1" value="vishu"> </constructor-arg>
    <constructor-arg index="2" value="vishu@mail.com"> </constructor-arg>
    </bean>

</beans>